		* Light *
		Accent3 = #98ecfe;
		Accent2 = #60ccfe;
		Accent1 = #0093f9;
		* Base *
		Accent0 = #0078d4;
		* Dark *
		Accent1  = #005eb7;
		Accent2  = #003d92;
		Accent3  = #001968;
		
		AccentOnAccent = MixColor(ReversedThemeColor(CurrentTheme.Accent, index),IsLightTheme?White:Black,0.7);
